{
  "name": "@f3rh47/gulp-starter-kit",
  "version": "1.0.0",
  "description": "A simple Gulp 4 Starter Kit for modern web development.",
  "keywords": [
    "gulp",
    "gulp-starter-kit",
    "pug",
    "sass",
    "babel",
    "imagemin",
    "eslint",
    "stylelint",
    "prettier",
    "lint-staged",
    "browser-sync",
    "autoprefixer",
    "modern-web-dev"
  ],
  "scripts": {
    "start": "gulp",
    "build": "gulp build",
    "lint:js": "eslint .",
    "lint:css": "stylelint 'src/scss/**/*.scss'",
    "lint": "yarn run lint:js && yarn run lint:css",
    "format:js": "prettier --write 'src/**/*.js'",
    "format:css": "prettier-stylelint --quiet --write 'src/scss/**/*.scss'",
    "format": "yarn run format:js && yarn run format:css",
    "clear": "gulp clear",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/f3rh47/gulp-starter-kit.git"
  },
  "author": "Ferhat Ä°ncekara <ferhatincekaraa@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/f3rh47/gulp-starter-kit/issues"
  },
  "homepage": "https://github.com/f3rh47/gulp-starter-kit",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-eslint": "^10.0.1",
    "browser-sync": "^2.26.3",
    "del": "^4.0.0",
    "eslint": "^5.15.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-pug": "^1.1.1",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-babel": "^8.0.0-beta.2",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-group-css-media-queries": "^1.2.2",
    "gulp-htmlmin": "^5.0.1",
    "gulp-imagemin": "^5.0.3",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.1",
    "gulp-pug": "^4.0.1",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "prettier-stylelint": "^0.4.2",
    "stylelint": "^9.10.1",
    "stylelint-config-prettier": "^5.0.0",
    "stylelint-config-sass-guidelines": "^5.3.0",
    "stylelint-declaration-block-no-ignored-properties": "^1.1.0",
    "stylelint-declaration-strict-value": "^1.1.2",
    "stylelint-order": "^2.1.0",
    "stylelint-scss": "^3.5.4"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "ie >= 10"
  ],
  "dependencies": {
    "normalize.css": "^8.0.1"
  },
  "lint-staged": {
    "*.js": [
      "yarn run format:js",
      "git add"
    ],
    "*.scss": [
      "yarn run format:css",
      "git add"
    ]
  }
}
